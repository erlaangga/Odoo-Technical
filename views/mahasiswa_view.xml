<openerp>
	<data>
		<record model="ir.ui.view" id="mahasiswa_view_tree">
			<field name="name">mahasiswa Tree View</field>
			<field name="model">data.mahasiswa</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="id_mahasiswa" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="mahasiswa_view_form">
			<field name="name">mahasiswa Form View</field>
			<field name="model">data.mahasiswa</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name="name" />
							<field name="id_mahasiswa" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_view_mahasiswa">
			<field name="name">Mahasiswa</field>
			<field name="res_model">data.mahasiswa</field>
			<field name="view_mode">tree,form</field>
		</record>

	</data>
</openerp>